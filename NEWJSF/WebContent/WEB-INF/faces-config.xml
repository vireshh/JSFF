<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">

<managed-bean>
<managed-bean-name>helloBeanId</managed-bean-name>
<managed-bean-class>helloBean</managed-bean-class>
<managed-bean-scope>session</managed-bean-scope>
<managed-property>
<property-name>hibean</property-name>
<value>#{hibeanId}
</value>

</managed-property>
</managed-bean>

<managed-bean>
<managed-bean-name>hibeanId</managed-bean-name>
<managed-bean-class>hiBean</managed-bean-class>
<managed-bean-scope>session</managed-bean-scope>
</managed-bean>
</faces-config>
